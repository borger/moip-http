{
    "name": "prothos/moip",
    "description": "MOIP Payments Integration via HTTP - Package optimized for Laravel 5",
    "keywords": ["moip", "gateway", "payment", "laravel", "checkout", "intermediary"],
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "rmccue/requests": ">=1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Prothos\\Moip\\": "src/"
        },
        "files": [
            "src/Helper/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Prothos\\Moip\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Prothos\\Moip\\Providers\\MoipServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Borges",
            "email": "rafaelborks@me.com"
        }
    ]
}
